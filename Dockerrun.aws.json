{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinations" :  [
        {
            "name": "client",
            "image": "masettyabhishek/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "masettyabhishek/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "masettyabhishek/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name":"nginx",
            "image":"masettyabhishek/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMapping": [
                {
                    "hostPort":80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
            
        }

    ]
}